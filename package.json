{
  "name": "calculatrice",
  "version": "1.0.0",
  "description": "100% typescript and SASS",
  "main": "index.js",
  "scripts": {
    "build:prod": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "watch:prod": "webpack --watch --mode=production",
    "watch:dev": "webpack --watch --mode=development",
    "start:server": "webpack serve --mode development",
    "sass:dev": "sass assets/scss:public/build/css",
    "sass:prod": "sass --no-source-map assets/scss:public/build/css",
    "sass:watch": "sass --watch assets/scss:public/build/css",
    "build:ts": "tsc",
    "watch:ts": "tsc --watch"
  },
  "keywords": [],
  "author": "",
