  },
  "homepage": "https://github.com/Elogeek/calculatrice#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "mini-css-extract-plugin": "^2.4.6",
    "sass": "^1.44.0",
    "sass-loader": "^12.3.0",
    "scss-loader": "^0.0.1",
